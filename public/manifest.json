{
  "manifest_version": 3,
  "name": "Coupon Clipper",
  "version": "0.1.0",
  "description": "A basic Chrome extension for conveniently clipping all coupons on a page.",
  "permissions": ["scripting", "storage", "activeTab", "tabs"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
